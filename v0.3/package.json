{
  "name": "lunchboxjs-alpha",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:tsc": "tsc --project ./config/tsconfig.lib.json",
    "build:rollup": "rollup -c ./config/lib-rollup.js",
    "build:dts": "node utils/cp utils/lib-dts.d.ts dist/lunchboxjs.es.d.ts && node utils/cp utils/lib-dts.d.ts dist/lunchboxjs.umd.d.ts",
    "build:lib": "node utils/rm js && npm run build:tsc && npm run build:rollup && npm run build:dts"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lunchboxjs.umd.cjs",
  "module": "./dist/lunchboxjs.js",
  "exports": {
    ".": {
      "import": "./dist/lunchboxjs.js",
      "require": "./dist/lunchboxjs.umd.cjs"
    }
  },
  "dependencies": {
    "nodemon": "^3.1.0",
    "three": "^0.164.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/three": "^0.164.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vue-tsc": "^2.0.6"
  }
}